{
    "name": "lox-language",
    "displayName": "lox-language",
    "description": "Syntax highlighting for everyone's favorite interpreted learning-language",
    "version": "0.0.1",
    "publisher": "dberezin",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lox",
            "aliases": ["lox", "lox"],
            "extensions": [".lox"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lox",
            "scopeName": "source.lox",
            "path": "./syntaxes/lox.tmLanguage.json"
        }]
    }
}